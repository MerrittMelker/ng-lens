{
  "name": "ng-lens",
  "version": "1.0.0",
  "type": "module",
  "description": "Static analysis tool for Angular projects that maps DI services and method usage.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MerrittMelker/ng-lens.git"
  },
  "homepage": "https://github.com/MerrittMelker/ng-lens#readme",
  "bugs": {
    "url": "https://github.com/MerrittMelker/ng-lens/issues"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "npm run test:basic && npm run test:validation",
    "test:basic": "echo 'Running basic analysis test...' && ts-node src/index.ts",
    "test:validation": "echo 'Running validation tests...' && ts-node src/test-runner.ts",
    "scan:routes": "ts-node src/route-scan.ts"
  },
  "dependencies": {
    "ts-morph": "^22.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
